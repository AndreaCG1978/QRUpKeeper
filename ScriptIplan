      KEY `idForm` (`idForm`),
      CONSTRAINT `artefact_ibfk_11` FOREIGN KEY (`idForm`) REFERENCES `forms` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;


DROP TABLE IF EXISTS `aireAcond`; 

