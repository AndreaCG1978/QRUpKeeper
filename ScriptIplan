   CONSTRAINT `artefact_ibfk_18` FOREIGN KEY (`datacenterId`) REFERENCES `datacenters` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;


