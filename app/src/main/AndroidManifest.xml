<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.boxico.android.kn.qrupkeeper">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />






    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity android:name="com.boxico.android.kn.qrupkeeper.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
            </intent-filter>
        </activity>

        <activity android:name="com.boxico.android.kn.qrupkeeper.MainActivity" >
        </activity>

        <activity android:name="com.boxico.android.kn.qrupkeeper.util.ExpandableListFragment">
        </activity>

        <activity android:name="com.boxico.android.kn.qrupkeeper.util.QrScannerUtil">
        </activity>


    </application>

</manifest>